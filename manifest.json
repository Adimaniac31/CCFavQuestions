{
    "name": "CodeChef Favourite Questions",
    "version":"0.0.1",
    "manifest_version": 2,
    "browser_action":{
        "default_icon":"logo.png",
        "default_popup": "popup.html"
    },
    "content_scripts":  [
        {
            "matches":["http://www.codechef.com/*"],
            "js":["contentScript.js"],
            "run_at":"document_end"
        }
    ],
    "icons":{
        "128": "logo.png"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },    
    "permissions":[
                    "activeTab",
                    "clipboardWrite"
                  ]
}